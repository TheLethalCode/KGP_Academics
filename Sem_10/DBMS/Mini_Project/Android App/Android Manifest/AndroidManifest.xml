<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.event_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".manageEvents"></activity>
        <activity android:name=".viewRooms" />
        <activity android:name=".addEvent" />
        <activity android:name=".dashboard" />
        <activity android:name=".Em_login" />
        <activity android:name=".UserBookings" />
        <activity android:name=".UserProfile" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".RoomActivity" />
        <activity
            android:name=".AdminLoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".myevents" />
        <activity android:name=".Profile" />
        <activity android:name=".Events" />
        <activity android:name=".Register" />
        <activity android:name=".UserLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>